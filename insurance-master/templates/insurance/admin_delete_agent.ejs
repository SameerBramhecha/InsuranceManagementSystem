<%- include ("adminbase") %>
<head>
  <link
    href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css"
    rel="stylesheet"
    id="bootstrap-css"
  />
  <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
  <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

  <style media="screen">
    a:link {
      text-decoration: none;
    }

    h6 {
      text-align: center;
    }

    .row {
      margin: 100px;
    }
  </style>
</head>
<br /><br />
<div class="container" style="margin-top: -40%">
  <div class="panel panel-primary" style="margin-left: 25%; margin-right: 25%">
    <div class="panel-heading">
      <h6 class="panel-title">Delete Agent</h6>
    </div>
    <%if (records){ %>
    <table class="table table-hover" id="dev-table">
      <thead>
        <tr>
          <th>Agent Id </th>
          <th>Agent Name</th>
          <th>Contact</th>
          <th>Email Id</th>
          <th>Address</th>    
        </tr>
      </thead>
      <% records.forEach(function(ag){ %>
      <tr>
        <td><%= ag.Agent_Id %></td>
        <td><%= ag.Agent_Name %></td>
        <td><%= ag.Contact_No %></td>
        <td><%= ag.Email_Id %></td>
        <td><%= ag.Address %></td>
       
      </tr>
      <% }); %>
    </table>
    <%}%>
  </div>
</div>
<form action='/deleteagent/Id' method = "post">
    <div class="col"  style="margin-left: 30%; margin-right: 30%">
      <input type="text" name= "Agent_Id" class="form-control" placeholder="Enter Id to delete" />
    
    <button type="submit" class="btn btn-outline-primary">Delete Agent</button>
    </div>
</form>
<br /><br /><br /><br /><br /><br />
